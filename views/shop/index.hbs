{{# each movies }}
<div class="row">

{{# each this }}

  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{this.imagePath}}" alt="..."  class="img-responsive">
      <div class="caption">
        <h3>{{this.title}}</h3>
       <p class="description"> {{this.title}} is a {{this.year }} . It was written by  {{this.author}}.
        </p>
     <div class= "clearfix">
        <a href="#" class="btn btn-primary pull-right" role="button">Details</a> 
      </div>
      </div>
    </div>
  </div>
  {{/each}}
  
</div>
{{/each}}


  


